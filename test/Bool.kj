#use("Object.kj");

class Bool
{
	#atomField(int, ___data, 1);

	static method True()
	{
		return true;
	}

	static method False()
	{
		return false;
	}

	method Not()
	{
		var result = false;
		#inject("movi regi:0 :*$result->Bool/___Data.___data");
		#inject("not 0 0");
		#inject("movi :*$result->Bool/___Data.___data regi:0");
		return result;
	}
}
