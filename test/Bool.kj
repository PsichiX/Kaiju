#use("Object.kj");

class Bool
{
	#atomField(int, ___data, 1);

	static met True()
	{
		return true;
	}

	static met False()
	{
		return false;
	}

	met Not()
	{
		let result = false;
		#inject("movi regi:0 :*$result->Bool/___Data.___data");
		#inject("not 0 0");
		#inject("movi :*$result->Bool/___Data.___data regi:0");
		return result;
	}
}
