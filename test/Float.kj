#use("Object.kj");

class Float
{
	#atomField(float, ___data, 1);

	method Constructor()
	{
		#inject("movf :*$this->Float/___Data.___data $___ZEROF");
	}

	method Destructor()
	{
		#inject("movf :*$this->Float/___Data.___data $___ZEROF");
	}

	method Add(v)
	{
		#ensureType(v, Float);
		var result = 0.0;
		#inject("movf regf:0 :*$this->Float/___Data.___data");
		#inject("movf regf:1 :*$v->Float/___Data.___data");
		#inject("addf 2 0 1");
		#inject("movf :*$result->Float/___Data.___data regf:2");
		return result;
	}

	method Sub(v)
	{
		#ensureType(v, Float);
		var result = 0.0;
		#inject("movf regf:0 :*$this->Float/___Data.___data");
		#inject("movf regf:1 :*$v->Float/___Data.___data");
		#inject("subf 2 0 1");
		#inject("movf :*$result->Float/___Data.___data regf:2");
		return result;
	}

	method Mul(v)
	{
		#ensureType(v, Float);
		var result = 0.0;
		#inject("movf regf:0 :*$this->Float/___Data.___data");
		#inject("movf regf:1 :*$v->Float/___Data.___data");
		#inject("mulf 2 0 1");
		#inject("movf :*$result->Float/___Data.___data regf:2");
		return result;
	}

	method Div(v)
	{
		#ensureType(v, Float);
		var result = 0.0;
		#inject("movf regf:0 :*$this->Float/___Data.___data");
		#inject("movf regf:1 :*$v->Float/___Data.___data");
		#inject("divf 2 0 1");
		#inject("movf :*$result->Float/___Data.___data regf:2");
		return result;
	}
}
